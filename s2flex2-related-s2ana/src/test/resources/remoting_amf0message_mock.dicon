<?xml version="1.0" encoding="Shift_JIS"?>
<!DOCTYPE components PUBLIC "-//SEASAR//DTD S2Container 2.3//EN" 
	"http://www.seasar.org/dtd/components23.dtd">
<components namespace="remoting_amf0">
	<component
		class="org.seasar.flex2.rpc.remoting.message.data.MessageHeader"/>
		
	<component
		name="requestMessage"
		class="org.seasar.flex2.rpc.remoting.message.data.Message"
		instance="request">
		<initMethod name="addHeader">
			<arg>
				<component class="org.seasar.flex2.rpc.remoting.message.data.MessageHeader">
					<property name="name">
						@org.seasar.flex2.rpc.remoting.message.RemotingMessageConstants@HEADER_NAME_CREDENTINALS
					</property>
					<property name="value">
						<component class="org.seasar.flex2.core.format.amf0.type.Amf0Object" >
			   				<initMethod name="put">
   								<arg>"userid"</arg>
			   					<arg>"admin"</arg>
   							</initMethod>
   							<initMethod name="put">
   								<arg>"password"</arg>
			   					<arg>"adminpass"</arg>
   							</initMethod>
			   			</component>
					</property>
				</component>
			</arg>
		</initMethod>
	</component>

	<component
		class="org.seasar.flex2.rpc.remoting.message.data.factory.impl.MessageFactoryImpl" />

</components>